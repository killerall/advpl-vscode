{
	"Protheus doc" : {
			"prefix": "protheusdoc"
		,	"body": [
				"//-------------------------------------------------------------------"
			,	"/*/{Protheus.doc} ${function}"
			,	"${description}"
			,	"@author  ${author}"
			,	"@since   ${date}"
			,	"@version ${version}"
			,	"/*/"
			,	"//-------------------------------------------------------------------"
			]
		,	"description": "Protheus doc"
	},
		"If": {
		"prefix": "If",
		"body": [
		"If ${lCond}",
		"	$1",
		"EndIf"
		],
		"description": "If Statement"
	},
	
	"If-Else": {
		"prefix": "If-Else",
		"body": [
		"If ${lCond}",
		"	$1",
		"Else",
		"	",
		"EndIf"
		],
		"description": "If-Else Statement"
	},

	"If-ElseIf": {
		"prefix": "If-ElseIf",
		"body": [
		"If ${lCond1}",
		"	$1",
		"ElseIf ${lCond2}",
		"	",
		"EndIf"
		],
		"description": "If-ElseIf Statement"
	},
	
	"Case": {
		"prefix": "Case Statement",
		"body": [
		"Do Case",
		"	Case (${lCond})",
		"		$1",
		"	OtherWise	",
		"		",
		"EndCase"
		],
		"description": "Case Statement"
	},

	"While": {
		"prefix": "While Statement",
		"body": [
		"While (${lCond})",
		"	$1",
		"EndDo"
		],
		"description": "While Statement"
	},

	"For": {
		"prefix": "For Statement",
		"body": [
		"Local ${nX} := 0",
		"For ${nX}:= 1 to Len(${aArray})",
		"	$1",
		"Next"
		],
		"description": "For Statement"
	}
}