{
	"Protheus doc" : {
			"prefix": "protheusdoc"
		,	"body": [
				"//-------------------------------------------------------------------"
			,	"/*/{Protheus.doc} ${function}"
			,	"${description}"
			,	"@author  ${author}"
			,	"@since   ${date}"
			,	"@version ${version}"
			,	"/*/"
			,	"//-------------------------------------------------------------------"
			]
		,	"description": "Protheus doc"
	},
		"If": {
		"prefix": "if",
		"body": [
		"If ${lCond}",
		"	$1",
		"EndIf"
		],
		"description": "If Statement"
	},
	
	"If Else": {
		"prefix": "if else",
		"body": [
		"If ${lCond}",
		"	$1",
		"Else",
		"	",
		"EndIf"
		],
		"description": "If Else Statement"
	},

	"If ElseIf": {
		"prefix": "if elseif",
		"body": [
		"If ${lCond1}",
		"	$1",
		"ElseIf ${lCond2}",
		"	",
		"EndIf"
		],
		"description": "If ElseIf Statement"
	},
	
	"Case": {
		"prefix": "case statement",
		"body": [
		"Do Case",
		"	Case (${lCond})",
		"		$1",
		"	OtherWise	",
		"		",
		"EndCase"
		],
		"description": "Case Statement"
	},

	"While": {
		"prefix": "while statement",
		"body": [
		"While (${lCond})",
		"	$1",
		"EndDo"
		],
		"description": "While Statement"
	},

	"For": {
		"prefix": "for statement",
		"body": [
		"For ${nX}:= 1 to Len(${aArray})",
		"	$1",
		"Next"
		],
		"description": "For Statement"
	}
}
